#!/bin/sh

for n in 5000 10000 20000 40000 50000 60000 80000 90000 100000 150000 200000 250000
do
    # Generates Random List
    # Has duplicates
    ./randList $n > data/d_random_$n
    sort -n < data/d_random_$n > data/d_sorted_$n
    sort -nr < data/d_random_$n > data/d_reverse_$n
    # No duplicates
    seq $n > data/nd_sorted_$n
    sort -R < data/nd_sorted_$n > data/nd_random_$n
    sort -nr < data/nd_sorted_$n > data/nd_reverse_$n
    
    # Record Timing Data
    echo ============================================================================================================================================================
    echo ============================= N = $n =============================
    echo ====================== Random No Duplicates ======================
    echo ""
    for i in 1 2 3
    do
        echo - useIntList Trial $i:
        time ./useIntList < data/nd_random_$n > /dev/null
        echo ""
    done
    for i in 1 2 3
    do
        echo - sort Trial $i:
        time sort < data/nd_random_$n > /dev/null
        echo ""
    done

    echo ====================== Sorted No Duplicates ======================
    echo ""
    for i in 1 2 3
    do
        echo - useIntList Trial $i:
        time ./useIntList < data/nd_sorted_$n > /dev/null
        echo ""
    done
    for i in 1 2 3
    do
        echo - sort Trial $i:
        time sort < data/nd_sorted_$n > /dev/null
        echo ""
    done
    

    echo ====================== Reverse No Duplicates ======================
    echo ""
    for i in 1 2 3
    do
        echo - useIntList Trial $i:
        time ./useIntList < data/nd_reverse_$n > /dev/null
        echo ""
    done
    for i in 1 2 3
    do
        echo - sort Trial $i:
        time sort < data/nd_reverse_$n > /dev/null
        echo ""
    done


    echo ====================== Random With Duplicates ======================
    echo ""
    for i in 1 2 3
    do
        echo - useIntList Trial $i:
        time ./useIntList < data/d_random_$n > /dev/null
        echo ""
    done
    for i in 1 2 3
    do
        echo - sort Trial $i:
        time sort < data/d_random_$n > /dev/null
        echo ""
    done

    echo ====================== Sorted With Duplicates ======================
    echo ""
    for i in 1 2 3
    do
        echo - useIntList Trial $i:
        time ./useIntList < data/d_sorted_$n > /dev/null
        echo ""
    done
    for i in 1 2 3
    do
        echo - sort Trial $i:
        time sort < data/d_sorted_$n > /dev/null
        echo ""
    done

    echo ====================== Reverse With Duplicates ======================
    echo ""
    for i in 1 2 3
    do
        echo - useIntList Trial $i:
        time ./useIntList < data/d_reverse_$n > /dev/null
        echo ""
    done
    for i in 1 2 3
    do
        echo - sort Trial $i:
        time sort < data/d_reverse_$n > /dev/null
        echo ""
    done
    echo ""
    echo ""
done

